<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jazzdevstudio.lacunaexpress" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jazzdevstudio.lacunaexpress.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jazzdevstudio.lacunaexpress.AddAccount"
            android:label="@string/title_activity_add_account"
            android:parentActivityName="jazzdevstudio.lacunaexpress.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jazzdevstudio.lacunaexpress.MainActivity" />
        </activity>
        <activity
            android:name="jazzdevstudio.lacunaexpress.SelectAccount"
            android:label="@string/title_activity_select_account"
            android:parentActivityName="jazzdevstudio.lacunaexpress.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jazzdevstudio.lacunaexpress.MainActivity" />
        </activity>
    </application>

</manifest>
